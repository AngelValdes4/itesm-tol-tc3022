void setup(){
  size(500,700);
  background(255);
}

void draw(){
    myFilledShape();
    pushMatrix();
    scale(2.0);
    rotate(radians(45));
    //translate(-40, -40);
    myEmptyShape();
    popMatrix();
    
}

void transform(){
    
}

void myFilledShape(){
    fill(#039be5);
    noStroke();
    rect(40, 40, 40, 40);
}

void myEmptyShape(){
    noFill();
    stroke(#0069c0);
    rect(0, 0, 40, 40);
}
